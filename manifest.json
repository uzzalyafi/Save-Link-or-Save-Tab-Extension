{
  "manifest_version": 3,
  "name": "Save ALL links",
  "version": "1.0",
  "description": "A simple extension to get the current tab URL And Save ALL links.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png",
    "default_title": "Save Link"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "author": "Uzzal Yafi"

}

